# Multi-Agent Scheduler Environment Variables
# Copy this file to .env and fill in your actual values

# ===================================================================
# API Keys
# ===================================================================

# Anthropic Claude API Key
# Get your key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-...

# OpenAI API Key
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-...

# Google Gemini API Key
# Get your key from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=...

# ===================================================================
# Security
# ===================================================================

# Master password for API key encryption
# Use a strong, unique password
SECURITY_MASTER_PASSWORD=your-secure-master-password-here

# ===================================================================
# Configuration Overrides
# ===================================================================

# Override any config.yaml setting using SECTION_KEY format
# Examples:

# Scheduler settings
SCHEDULER_MAX_CONCURRENT=10
SCHEDULER_MAX_TASKS=50
SCHEDULER_DEFAULT_TIMEOUT=600

# Agent settings
AGENTS_CLAUDE_MODEL=claude-sonnet-4-5-20250929
AGENTS_CLAUDE_MAX_TOKENS=1024
AGENTS_OPENAI_MODEL=gpt-4-turbo

# Meta-agent settings
META_AGENT_MIN_TASKS=5
META_AGENT_MAX_TASKS=20
META_AGENT_AGENT_TYPE=claude

# Cache settings
CACHE_ENABLED=true
CACHE_MAX_SIZE=1000
CACHE_TTL=3600

# Logging settings
LOGGING_ENABLED=true
LOGGING_LOG_DIR=logs
LOGGING_LOG_LEVEL=INFO
LOGGING_LOG_RATIONALE=true

# Workspace settings
WORKSPACE_BASE_DIR=workspaces
WORKSPACE_USE_TIMESTAMPS=true

# Metrics settings
METRICS_ENABLED=true
METRICS_PRINT_STATS=true

# Plugin settings
PLUGINS_ENABLED=true
PLUGINS_PLUGIN_DIR=plugins

# ===================================================================
# Advanced Settings
# ===================================================================

# Connection pool
ADVANCED_CONNECTION_POOL_ENABLED=true
ADVANCED_CONNECTION_POOL_MAX_CONNECTIONS=50

# Retry settings
ADVANCED_RETRY_MAX_RETRIES=3
ADVANCED_RETRY_BACKOFF_MULTIPLIER=2
ADVANCED_RETRY_INITIAL_DELAY=1

# Rate limiting
ADVANCED_RATE_LIMIT_ENABLED=false
ADVANCED_RATE_LIMIT_REQUESTS_PER_MINUTE=60

# ===================================================================
# Development/Testing
# ===================================================================

# Set to "development" or "production"
ENVIRONMENT=development

# Enable debug mode
DEBUG=false

# Test mode (disables actual API calls)
TEST_MODE=false
