{
  "dashboard": {
    "id": null,
    "uid": "scheduler-main",
    "title": "Multi-Agent Scheduler - Overview",
    "tags": ["scheduler", "multi-agent"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "type": "graph",
        "title": "Task Execution Rate",
        "targets": [
          {
            "expr": "rate(scheduler_tasks_total[5m])",
            "legendFormat": "{{agent_type}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "tasks/sec", "show": true },
          { "label": "", "show": false }
        ],
        "lines": true,
        "fill": 1
      },
      {
        "id": 2,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "type": "stat",
        "title": "Active Tasks",
        "targets": [
          {
            "expr": "scheduler_tasks_active",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 3,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "type": "graph",
        "title": "Task Duration (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(scheduler_task_duration_seconds_bucket[5m]))",
            "legendFormat": "{{agent_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "seconds", "show": true },
          { "label": "", "show": false }
        ]
      },
      {
        "id": 4,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "type": "graph",
        "title": "Error Rate",
        "targets": [
          {
            "expr": "rate(scheduler_errors_total[5m])",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "errors/sec", "show": true },
          { "label": "", "show": false }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": { "params": [1], "type": "gt" },
              "operator": { "type": "and" },
              "query": { "params": ["A", "5m", "now"] },
              "reducer": { "params": [], "type": "avg" },
              "type": "query"
            }
          ],
          "frequency": "60s",
          "name": "High Error Rate"
        }
      },
      {
        "id": 5,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
        "type": "graph",
        "title": "Checkpoint Operations",
        "targets": [
          {
            "expr": "rate(checkpoint_operations_total[5m])",
            "legendFormat": "{{operation}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
        "type": "graph",
        "title": "Workflow Executions",
        "targets": [
          {
            "expr": "rate(workflow_executions_total[5m])",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ]
      }
    ]
  }
}
