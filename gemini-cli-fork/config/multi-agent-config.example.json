{
  "$schema": "./multi-agent-config-schema.json",
  "scheduler": {
    "type": "multi-agent",
    "comment": "Options: 'default' or 'multi-agent'",
    "debug": true,
    "fallbackOnError": true
  },
  "agents": {
    "claude": {
      "enabled": true,
      "apiKey": "${ANTHROPIC_API_KEY}",
      "model": "claude-sonnet-4-5-20250929",
      "maxConcurrent": 5,
      "timeout": 60000,
      "capabilities": [
        "complex-reasoning",
        "architecture-design",
        "code-review",
        "documentation"
      ],
      "costPerMillionTokens": {
        "input": 3.0,
        "output": 15.0
      }
    },
    "openai": {
      "enabled": true,
      "apiKey": "${OPENAI_API_KEY}",
      "model": "gpt-4-turbo",
      "maxConcurrent": 3,
      "timeout": 60000,
      "capabilities": [
        "code-generation",
        "api-development",
        "refactoring",
        "debugging"
      ],
      "costPerMillionTokens": {
        "input": 10.0,
        "output": 30.0
      }
    },
    "gemini": {
      "enabled": true,
      "apiKey": "${GOOGLE_API_KEY}",
      "model": "gemini-2.5-pro",
      "maxConcurrent": 5,
      "timeout": 60000,
      "capabilities": [
        "testing",
        "simple-tasks",
        "data-processing",
        "analysis"
      ],
      "costPerMillionTokens": {
        "input": 0.125,
        "output": 0.375
      }
    },
    "codex": {
      "enabled": false,
      "comment": "Codex CLI agent - currently not implemented",
      "command": "codex",
      "capabilities": [
        "shell-commands",
        "system-tasks"
      ]
    }
  },
  "metaAgent": {
    "model": "claude-sonnet-4-5-20250929",
    "provider": "claude",
    "systemPrompt": "You are a Meta-Agent that decomposes complex tasks into subtasks and assigns them to the best available agent.",
    "optimizationStrategy": "balanced",
    "comment": "Options: 'speed', 'cost', 'quality', or 'balanced'"
  },
  "execution": {
    "parallelism": {
      "enabled": true,
      "maxParallelTasks": 5
    },
    "retryPolicy": {
      "enabled": true,
      "maxRetries": 2,
      "backoffMultiplier": 2,
      "initialDelayMs": 1000
    },
    "timeout": {
      "decomposition": 30000,
      "taskExecution": 60000,
      "total": 300000
    }
  },
  "ui": {
    "showPlan": true,
    "requireConfirmation": false,
    "streamOutput": true,
    "verboseLogging": true,
    "colorScheme": "multi-agent"
  },
  "telemetry": {
    "enabled": true,
    "trackCosts": true,
    "trackLatency": true,
    "trackAgentPerformance": true
  }
}
